extends ../../../../../app/views/_layout

block append meta
	meta(name="ol-admin-project-data" data-type="json" content=project)

block content
	.content.content-alt(ng-controller="AdminProjectController")
		.container(ng-cloak)
			p
				strong
					| Project
				| &nbsp;&gt;&nbsp;
				strong {{ project._id }}
			tabset
				tab(heading="Project Info")
					include ./_project-info
