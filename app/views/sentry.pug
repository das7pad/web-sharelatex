- if (sentryEnabled)
	script(type="text/javascript").
		function initSentry() {
			if (typeof Sentry !== "undefined") {
				Sentry.init(JSON.parse('!{JSON.stringify(settings.sentry.frontend)}'))
				Sentry.setUser({id:'#{getLoggedInUserId()}'})
			}
		}
	script(async defer src=sentrySRC, crossorigin="anonymous" onload='initSentry()')
